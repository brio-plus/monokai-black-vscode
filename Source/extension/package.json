{
  "name": "theme-monokai-black-vscode",
  "displayName": "Monokai Black",
  "description": "Monokai Black theme with syntax-highlighted markdown preview.",
  "publisher": "brio-plus",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Themes"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "themes": [
      {
        "label": "Monokai Black",
        "uiTheme": "vs-dark",
        "path": "./themes/Monokai Black.json"
      }
    ],
    "markdown.previewStyles": [
      "./media/markdown-preview.css"
    ],
    "markdown.markdownItPlugins": true
  },
  "scripts": {
    "compile": "node esbuild.config.mjs",
    "watch": "node esbuild.config.mjs --watch",
    "build": "node esbuild.config.mjs --production",
    "package": "npm run build && vsce package --no-dependencies --out ../../build/",
    "vscode:prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.79.0",
    "@vscode/vsce": "^2.22.0",
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "shiki": "^1.0.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "homepage": "https://brio.plus",
  "bugs": {
    "url": "https://github.com/brio-plus/monokai-black-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brio-plus/monokai-black-vscode.git"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "icon": "icon.png",
  "keywords": [
    "beautiful functionality",
    "focus",
    "minimal",
    "syntax highlighting",
    "monokai",
    "dark theme",
    "markdown preview"
  ],
  "qna": false
}
