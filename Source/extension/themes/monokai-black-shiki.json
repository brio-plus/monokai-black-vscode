{
  "name": "Monokai Black",
  "type": "dark",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#f7f1ff"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "comment keyword",
        "comment markup.underline.link",
        "comment string",
        "comment punctuation.definition",
        "comment punctuation",
        "comment text",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#69676c"
      }
    },
    {
      "scope": [
        "string.quoted.docstring",
        "string.quoted.docstring punctuation.definition"
      ],
      "settings": {
        "foreground": "#69676c"
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.language",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#948ae3"
      }
    },
    {
      "scope": "constant.other",
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": [
        "constant.other.caps",
        "constant.other.property",
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#948ae3"
      }
    },
    {
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#fd9353"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.templated",
        "entity.name.function.member.static",
        "support.function",
        "meta.function-call.generic.python",
        "support.function.builtin.python"
      ],
      "settings": {
        "foreground": "#7bd88f"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.type.class.templated",
        "entity.name.type.class.generic",
        "entity.name.type.namespace",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.type.object.console"
      ],
      "settings": {
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.name.tag.js.jsx support.class.component.js.jsx"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.parent-selector-suffix.css"
      ],
      "settings": {
        "foreground": "#7bd88f"
      }
    },
    {
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#fd9353"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.pseudo-class.css",
        "entity.other.pseudo-element.css"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.operator.member",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": [
        "keyword.other.template.begin",
        "keyword.other.template.end"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#fce566"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#fd9353"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#7bd88f"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "markup.inserted punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#7bd88f"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "markup.deleted punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": [
        "markup.changed",
        "markup.changed punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#fce566"
      }
    },
    {
      "scope": [
        "meta.brace.round",
        "meta.brace.square",
        "meta.brace.curly",
        "meta.delimiter.comma.js"
      ],
      "settings": {
        "foreground": "#8b888f"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#8b888f"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag source"
      ],
      "settings": {
        "foreground": "#8b888f"
      }
    },
    {
      "scope": [
        "punctuation.template-string.element.begin",
        "punctuation.template-string.element.end",
        "punctuation.definition.string.template.begin",
        "punctuation.definition.string.template.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "source",
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": "source.json meta.mapping.key string",
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": "source.yaml meta.mapping.key string",
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": [
        "storage",
        "storage.modifier",
        "storage.type.modifier"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": "storage.type.function.arrow",
      "settings": {
        "fontStyle": "normal",
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#fce566"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": [
        "string.other.link.title",
        "string.other.link.description"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "string.other.link.description.title",
      "settings": {
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "string.regexp punctuation.definition.string.begin",
        "string.regexp punctuation.definition.string.end"
      ],
      "settings": {
        "foreground": "#fc618d"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "fontStyle": "normal",
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": "text",
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "variable.other.readwrite",
        "variable.other.member",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#f7f1ff"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "parameters variable.function"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fd9353"
      }
    },
    {
      "scope": "variable.parameter.tql",
      "settings": {
        "fontStyle": "normal",
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "variable.language",
        "variable.parameter.function.language.special.self.python",
        "variable.parameter.function.language.special.cls.python"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bab6c0"
      }
    },
    {
      "scope": "variable.other.class",
      "settings": {
        "foreground": "#5ad4e6"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#948ae3"
      }
    },
    {
      "scope": "variable.function",
      "settings": {
        "foreground": "#7bd88f"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#fc618d",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#fd9353",
        "fontStyle": "italic underline"
      }
    }
  ]
}
